#!/usr/bin/env node
    var _ = require('lodash');
    var allowedOptions = require('./../lib/options');
    var CloudflareCli = require('./../index');
    var ConfigReader = require('./../lib/configReader');
    var config =  new ConfigReader().readConfig('~/.cfcli.yml');
    var argv = _.extend(config, require('minimist')(process.argv.slice(2), allowedOptions));
    var cli = new CloudflareCli(argv);

    cli.runCommand(argv._[0], argv);
